              <group delimiter=", ">
                <text variable="volume" font-style="normal" prefix="vol. &lt;volume>" suffix="&lt;/volume>"/>
              </group>
              <group>
                <label suffix=" " variable="page" form="short"/>
                <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
              </group>
            </group>
