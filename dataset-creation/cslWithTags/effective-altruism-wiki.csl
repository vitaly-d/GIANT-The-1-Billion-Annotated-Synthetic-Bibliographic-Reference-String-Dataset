          <else>
            <group delimiter=", " suffix=".">
              <date delimiter=" " variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
                <date-part name="month" prefix="&lt;month>" suffix="&lt;/month>"/>
                <date-part name="day" prefix="&lt;day>" suffix="&lt;/day>"/>
              </date>
              <group delimiter=" ">
                <label variable="page" form="short"/>
                <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
              </group>
            </group>
          </else>
        </choose>
      </else-if>
      <else-if type="article-newspaper">
        <group delimiter=", " suffix=".">
          <date delimiter=" " variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
            <date-part name="month" prefix="&lt;month>" suffix="&lt;/month>"/>
            <date-part name="day" prefix="&lt;day>" suffix="&lt;/day>"/>
          </date>
          <group delimiter=" ">
            <label variable="page" form="short"/>
            <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
          </group>
        </group>
      </else-if>
      <else-if type="speech">
        <group delimiter=", " suffix=".">
          <group>
            <text variable="publisher-place" prefix="&lt;publisher-place>" suffix="&lt;/publisher-place>"/>
          </group>
          <group>
            <date delimiter=" " variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
              <date-part name="month" prefix="&lt;month>" suffix="&lt;/month>"/>
              <date-part name="day" prefix="&lt;day>" suffix="&lt;/day>"/>
            </date>
          </group>
        </group>
      </else-if>
      <else-if type="bill broadcast interview legal_case legislation patent post post-weblog speech treaty webpage" match="none">
        <text macro="locators-booklike" prefix="" suffix=""/>
      </else-if>
      <else-if type="post post-weblog webpage broadcast report" match="any">
        <choose>
          <if variable="number" match="any">
            <group delimiter=", " suffix=".">
              <group>
                <text variable="container-title" text-case="title" font-style="normal" prefix="*&lt;container-title>" suffix="&lt;/container-title>*"/>
              </group>
              <group>
                <date delimiter=" " variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
                  <date-part name="month" suffix="&lt;/month> " prefix="&lt;month>"/>
                  <date-part name="day" suffix="&lt;/day> " prefix="&lt;day>"/>
                </date>
                <text variable="number" font-style="normal" prefix="(updated &lt;number>" suffix="&lt;/number>)"/>
              </group>
            </group>
          </if>
          <else>
            <group delimiter=", " suffix=".">
              <group>
                <text variable="container-title" text-case="title" font-style="normal" prefix="*&lt;container-title>" suffix="&lt;/container-title>*"/>
              </group>
              <group>
                <date delimiter=" " variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
                  <date-part name="month" prefix="&lt;month>" suffix="&lt;/month>"/>
                  <date-part name="day" suffix="&lt;/day>." prefix="&lt;day>"/>
                </date>
              </group>
            </group>
          </else>
        </choose>
      </else-if>
    </choose>
