      </if>
      <else-if type="article-newspaper article-magazine" match="any">
        <choose>
          <if variable="volume issue page issued" match="none">
            <text variable="container-title" font-style="normal" text-case="title" prefix="*&lt;container-title>" suffix="&lt;/container-title>*."/>
          </if>
          <else>
            <text variable="container-title" font-style="normal" text-case="title" prefix="*&lt;container-title>" suffix="&lt;/container-title>*"/>
          </else>
        </choose>
      </else-if>
      <else-if type="bill legal_case legislation post post-weblog webpage" match="none">
        <text variable="container-title" text-case="title" strip-periods="false" font-style="normal" prefix="*&lt;container-title>" suffix="&lt;/container-title>*,"/>
      </else-if>
      <else-if type="post post-weblog webpage broadcast" match="any">
        <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
      </else-if>
    </choose>
  </macro>
  <citation et-al-min="6" et-al-use-first="1" et-al-subsequent-min="3" et-al-subsequent-use-first="1" disambiguate-add-names="true" disambiguate-add-givenname="true" collapse="year" givenname-disambiguation-rule="primary-name">
    
    <layout prefix="(" suffix=")" delimiter=", ">
