  <locale xml:lang="en">
    <terms>
      <term name="editortranslator" form="short">
        <single>ed. &amp; trans.</single>
        <multiple>eds. &amp; trans.</multiple>
      </term>
      <term name="translator" form="short">trans.</term>
      <term name="interviewer" form="short">interviewer</term>
      <term name="circa" form="short">ca.</term>
      <term name="collection-editor" form="short">series ed.</term>
      <term name="et-al"> *et al.*</term>
    </terms>
  </locale>
  <macro name="container-contributors-booklike">
    <choose>
      <if variable="container-title">
        <names variable="editor translator" delimiter=", &amp; " prefix="&lt;editor translator>" suffix="&lt;/editor translator>">
          <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <label form="short" text-case="lowercase" prefix=" (" suffix=")"/>
          <substitute>
            <names variable="editorial-director" prefix="&lt;editorial-director>" suffix="&lt;/editorial-director>"/>
            <names variable="collection-editor" prefix="&lt;collection-editor>" suffix="&lt;/collection-editor>"/>
            <names variable="container-author" prefix="&lt;container-author>" suffix="&lt;/container-author>"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="container-contributors">
    <choose>
      <if type="book chapter entry entry-dictionary entry-encyclopedia graphic map personal_communication report speech paper-conference" match="any">
        <text macro="container-contributors-booklike" prefix="" suffix=""/>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors-booklike">
    <group delimiter=", ">
      <choose>
        <if variable="title">
          <names variable="interviewer" prefix="&lt;interviewer>" suffix="&lt;/interviewer>">
            <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
            <label form="short" prefix=", " text-case="title"/>
          </names>
        </if>
      </choose>
      <choose>
        <if variable="container-title" match="none">
          <group delimiter=", " suffix=",">
            <names variable="container-author" prefix="&lt;container-author>" suffix="&lt;/container-author>">
              <label form="verb" text-case="title" suffix=" "/>
              <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
            </names>
            <names variable="editor translator" delimiter=", " prefix="&lt;editor translator>" suffix="&lt;/editor translator>">
              <label form="verb" text-case="lowercase" suffix=" "/>
              <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
            </names>
          </group>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if type="book chapter entry entry-dictionary entry-encyclopedia graphic map report paper-conference" match="any">
        <text macro="secondary-contributors-booklike" prefix="" suffix=""/>
      </if>
      <else-if type="personal_communication">
        <group delimiter=", ">
          <group delimiter=" ">
            <choose>
              <if variable="genre" match="any">
                <text variable="genre" text-case="capitalize-first" prefix="&lt;genre>" suffix="&lt;/genre>"/>
              </if>
              <else>
                <text term="letter" text-case="capitalize-first" prefix="" suffix=""/>
              </else>
            </choose>
            <names variable="recipient" delimiter=", " prefix="&lt;recipient>" suffix="&lt;/recipient>">
              <label form="verb" suffix=" "/>
              <name and="symbol" delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
            </names>
          </group>
          <text variable="medium" text-case="capitalize-first" prefix="&lt;medium>" suffix="&lt;/medium>"/>
          <choose>
            <if variable="container-title" match="none">
              <names variable="editor translator" delimiter=", " prefix="&lt;editor translator>" suffix="&lt;/editor translator>">
                <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
                <label form="short" prefix=", " text-case="title"/>
              </names>
            </if>
          </choose>
        </group>
      </else-if>
      <else-if type="article-journal article-magazine article-newspaper" match="any">
        <group delimiter=", ">
          <choose>
            <if variable="title">
              <names variable="interviewer" delimiter=", " prefix="&lt;interviewer>" suffix="&lt;/interviewer>">
                <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
                <label form="short" prefix=", " text-case="title"/>
              </names>
            </if>
          </choose>
          <names variable="translator" delimiter=", " prefix="&lt;translator>" suffix="&lt;/translator>">
            <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
            <label form="short" prefix=", " text-case="title"/>
          </names>
        </group>
      </else-if>
      <else>
        <group delimiter=", ">
          <choose>
            <if variable="title">
              <names variable="interviewer" prefix="&lt;interviewer>" suffix="&lt;/interviewer>">
                <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
                <label form="short" prefix=", " text-case="title"/>
              </names>
            </if>
          </choose>
          <names variable="editor translator" delimiter=", " prefix="&lt;editor translator>" suffix="&lt;/editor translator>">
            <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
            <label form="short" prefix=", " text-case="title"/>
          </names>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author" delimiter=", " prefix="&lt;author>" suffix="&lt;/author>">
      <name and="symbol" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="1" initialize="true" initialize-with=". " name-as-sort-order="first"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="illustrator" prefix="&lt;illustrator>" suffix="&lt;/illustrator>"/>
        <names variable="composer" prefix="&lt;composer>" suffix="&lt;/composer>"/>
        <names variable="director" prefix="&lt;director>" suffix="&lt;/director>">
          <name name-as-sort-order="all" and="symbol" sort-separator=", " initialize-with=". " delimiter=", " delimiter-precedes-last="always"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <label form="long" prefix=" (" suffix=")" text-case="title"/>
        </names>
        <choose>
          <if variable="container-title">
            <choose>
              <if type="book entry entry-dictionary entry-encyclopedia">
                <text macro="title" prefix="" suffix=""/>
              </if>
              <else>
                <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>"/>
              </else>
            </choose>
            <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>">
              <name and="symbol" delimiter-precedes-last="always" initialize-with=". " name-as-sort-order="all"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
              <label form="short" prefix=" (" suffix=")" text-case="title"/>
            </names>
          </if>
        </choose>
        <names variable="editor" delimiter=", " prefix="&lt;editor>" suffix="&lt;/editor>">
          <name and="symbol" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="1" initialize-with=". " name-as-sort-order="first"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <label form="short" text-case="lowercase" prefix=" (" suffix=")"/>
        </names>
        <names variable="editorial-director" prefix="&lt;editorial-director>" suffix="&lt;/editorial-director>">
          <name name-as-sort-order="all" and="symbol" sort-separator=", " initialize-with=". " delimiter=", " delimiter-precedes-last="always"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <label form="short" prefix=" (" suffix=")" text-case="title"/>
        </names>
        <names variable="collection-editor" prefix="&lt;collection-editor>" suffix="&lt;/collection-editor>">
          <name name-as-sort-order="all" and="symbol" sort-separator=", " initialize-with=". " delimiter=", " delimiter-precedes-last="always"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <label form="short" prefix=" (" suffix=")" text-case="title"/>
        </names>
        <choose>
          <if type="report">
            <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
          </if>
        </choose>
        <group delimiter=" ">
          <text macro="title" prefix="" suffix=""/>
          <text macro="description" prefix="" suffix=""/>
          <text macro="format" prefix="" suffix=""/>
        </group>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <choose>
      <if type="personal_communication">
        <choose>
          <if variable="archive DOI ISBN publisher URL" match="none">
            <group delimiter=", ">
              <names variable="author" prefix="&lt;author>" suffix="&lt;/author>">
                <name and="symbol" delimiter=", " initialize-with=". "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
                <substitute>
                  <text variable="title" form="short" quotes="true" prefix="&lt;title>" suffix="&lt;/title>"/>
                </substitute>
              </names>
              <text value="personal communication" prefix="" suffix=""/>
            </group>
          </if>
          <else>
            <names variable="author" delimiter=", " prefix="&lt;author>" suffix="&lt;/author>">
              <name form="short" and="symbol" delimiter=", " initialize-with=". "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
              <substitute>
                <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>"/>
                <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>"/>
                <choose>
                  <if variable="container-title">
                    <text variable="title" form="short" quotes="true" prefix="&lt;title>" suffix="&lt;/title>"/>
                  </if>
                  <else>
                    <text variable="title" form="short" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
                  </else>
                </choose>
                <text macro="format-short" prefix="[" suffix="]"/>
              </substitute>
            </names>
          </else>
        </choose>
      </if>
      <else>
        <names variable="author" delimiter=", " prefix="&lt;author>" suffix="&lt;/author>">
          <name form="short" and="symbol" delimiter=", " initialize-with=". "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <substitute>
            <names variable="illustrator" prefix="&lt;illustrator>" suffix="&lt;/illustrator>"/>
            <names variable="composer" prefix="&lt;composer>" suffix="&lt;/composer>"/>
            <names variable="director" prefix="&lt;director>" suffix="&lt;/director>"/>
            <choose>
              <if variable="container-title">
                <choose>
                  <if type="book entry entry-dictionary entry-encyclopedia">
                    <text variable="title" form="short" quotes="true" prefix="&lt;title>" suffix="&lt;/title>"/>
                  </if>
                  <else>
                    <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>"/>
                  </else>
                </choose>
              </if>
            </choose>
            <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>"/>
            <names variable="editorial-director" prefix="&lt;editorial-director>" suffix="&lt;/editorial-director>"/>
            <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>"/>
            <choose>
              <if type="report" variable="publisher" match="all">
                <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
              </if>
              <else-if type="legal_case">
                <text variable="title" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
              </else-if>
              <else-if type="bill legislation" match="any">
                <text variable="title" form="short" prefix="&lt;title>" suffix="&lt;/title>"/>
              </else-if>
              <else-if variable="reviewed-author" type="review review-book" match="any">
                <text macro="format-short" prefix="[" suffix="]"/>
              </else-if>
              <else-if type="post post-weblog webpage broadcast" variable="container-title" match="any">
                <text variable="title" form="short" quotes="false" prefix="&lt;title>" suffix="&lt;/title>"/>
              </else-if>
              <else>
                <text variable="title" form="short" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
              </else>
            </choose>
            <text macro="format-short" prefix="[" suffix="]"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="archive">
        <choose>
          <if type="article article-journal article-magazine article-newspaper dataset paper-conference report speech thesis" match="any">
            <choose>
              <if variable="archive-place" match="none">
                <group delimiter=" ">
                  <text term="retrieved" text-case="capitalize-first" prefix="" suffix=""/>
                  <text term="from" prefix="" suffix=""/>
                  <text variable="archive" suffix="&lt;/archive>." prefix="&lt;archive>"/>
                  <text variable="archive_location" prefix="(&lt;archive_location>" suffix="&lt;/archive_location>)"/>
                </group>
              </if>
              <else>
                <text macro="publisher" suffix="." prefix=""/>
              </else>
            </choose>
          </if>
          <else>
            <text macro="publisher" suffix="." prefix=""/>
          </else>
        </choose>
      </if>
      <else>
        <text macro="publisher" suffix="." prefix=""/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if variable="title">
        <choose>
          <if variable="version" type="book" match="all">
            <text variable="title" prefix="&lt;title>" suffix="&lt;/title>"/>
          </if>
          <else-if variable="reviewed-author reviewed-title" type="review review-book" match="any">
            <choose>
              <if variable="reviewed-title">
                <choose>
                  <if type="post post-weblog webpage broadcast" variable="container-title" match="any">
                    <text variable="title" prefix="&lt;title>" suffix="&lt;/title>"/>
                  </if>
                  <else>
                    <text variable="title" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
                  </else>
                </choose>
              </if>
            </choose>
          </else-if>
          <else-if type="manuscript chapter" match="any">
            <choose>
              <if variable="note" match="any">
                <choose>
                  <if match="any" variable="DOI">
                    <text variable="title" text-case="title" font-style="normal" prefix="*[&lt;title>" suffix="&lt;/title>]"/>
                    <text variable="DOI" prefix="(http://doi.org/&lt;DOI>" suffix="&lt;/DOI>)*,"/>
                  </if>
                  <else-if match="any" variable="ISBN">
                    <text variable="title" text-case="title" font-style="normal" prefix="*[&lt;title>" suffix="&lt;/title>]"/>
                    <text variable="ISBN" prefix="(https://en.wikipedia.org/wiki/Special:BookSources/&lt;ISBN>" suffix="&lt;/ISBN>)*,"/>
                  </else-if>
                  <else-if match="any" variable="URL">
                    <text variable="title" text-case="title" font-style="normal" prefix="*[&lt;title>" suffix="&lt;/title>]"/>
                    <text variable="URL" prefix="(&lt;URL>" suffix="&lt;/URL>)*,"/>
                  </else-if>
                  <else>
                    <text variable="title" text-case="title" font-style="normal" prefix="*&lt;title>" suffix="&lt;/title>*,"/>
                  </else>
                </choose>
              </if>
              <else>
                <choose>
                  <if match="any" variable="DOI">
                    <text variable="title" prefix="[&lt;title>" suffix="&lt;/title>]"/>
                    <text variable="DOI" prefix="(http://doi.org/&lt;DOI>" suffix="&lt;/DOI>),"/>
                  </if>
                  <else-if match="any" variable="URL">
                    <text variable="title" prefix="[&lt;title>" suffix="&lt;/title>]"/>
                    <text variable="URL" prefix="(&lt;URL>" suffix="&lt;/URL>),"/>
                  </else-if>
                  <else-if match="any" variable="ISBN">
                    <text variable="title" prefix="[&lt;title>" suffix="&lt;/title>]"/>
                    <text variable="ISBN" prefix="(https://en.wikipedia.org/wiki/Special:BookSources/&lt;ISBN>" suffix="&lt;/ISBN>),"/>
                  </else-if>
                  <else>
                    <text variable="title" prefix="&amp;amp;apos;&lt;title>" suffix="&lt;/title>&amp;amp;apos;,"/>
                  </else>
                </choose>
              </else>
            </choose>
          </else-if>
          <else-if type="post post-weblog webpage broadcast report article-journal article-magazine article-newspaper paper-conference speech" match="any">
            <choose>
              <if match="any" variable="DOI">
                <text variable="title" prefix="[&lt;title>" suffix="&lt;/title>]"/>
                <text variable="DOI" prefix="(http://doi.org/&lt;DOI>" suffix="&lt;/DOI>),"/>
              </if>
              <else-if match="any" variable="URL">
                <text variable="title" prefix="[&lt;title>" suffix="&lt;/title>]"/>
                <text variable="URL" prefix="(&lt;URL>" suffix="&lt;/URL>),"/>
              </else-if>
              <else-if match="any" variable="ISBN">
                <text variable="title" prefix="[&lt;title>" suffix="&lt;/title>]"/>
                <text variable="ISBN" prefix="(https://en.wikipedia.org/wiki/Special:BookSources/&lt;ISBN>" suffix="&lt;/ISBN>),"/>
              </else-if>
              <else>
                <text variable="title" prefix="&amp;amp;apos;&lt;title>" suffix="&lt;/title>&amp;amp;apos;,"/>
              </else>
            </choose>
          </else-if>
          <else>
            <choose>
              <if match="any" variable="DOI">
                <text variable="title" text-case="title" font-style="normal" prefix="*[&lt;title>" suffix="&lt;/title>]"/>
                <text variable="DOI" prefix="(http://doi.org/&lt;DOI>" suffix="&lt;/DOI>)*,"/>
              </if>
              <else-if match="any" variable="ISBN">
                <text variable="title" text-case="title" font-style="normal" prefix="*[&lt;title>" suffix="&lt;/title>]"/>
                <text variable="ISBN" prefix="(https://en.wikipedia.org/wiki/Special:BookSources/&lt;ISBN>" suffix="&lt;/ISBN>)*,"/>
              </else-if>
              <else-if match="any" variable="URL">
                <text variable="title" text-case="title" font-style="normal" prefix="*[&lt;title>" suffix="&lt;/title>]"/>
                <text variable="URL" prefix="(&lt;URL>" suffix="&lt;/URL>)*,"/>
              </else-if>
              <else>
                <text variable="title" text-case="title" font-style="normal" prefix="*&lt;title>" suffix="&lt;/title>*,"/>
              </else>
            </choose>
          </else>
        </choose>
      </if>
      <else-if variable="interviewer" type="interview" match="any">
        <names variable="interviewer" prefix="&lt;interviewer>" suffix="&lt;/interviewer>">
          <label form="verb-short" suffix=" " text-case="capitalize-first"/>
          <name and="symbol" initialize-with=". " delimiter=", "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
        </names>
      </else-if>
    </choose>
  </macro>
  <macro name="description">
    <group>
      <choose>
        <if type="book report" match="any">
          <choose>
            <if variable="container-title">
              <text macro="secondary-contributors" prefix="" suffix=""/>
            </if>
            <else>
              <group delimiter=", ">
                <text macro="description-report" prefix="" suffix=""/>
                <text macro="secondary-contributors" prefix="" suffix=""/>
              </group>
            </else>
          </choose>
        </if>
        <else-if type="thesis">
          <group delimiter=", ">
            <group delimiter=", ">
              <text variable="genre" suffix="&lt;/genre>," prefix="&lt;genre>"/>
            </group>
            <text macro="locators" prefix="" suffix=""/>
            <text macro="secondary-contributors" prefix="" suffix=""/>
          </group>
        </else-if>
        <else-if type="book interview manuscript motion_picture musical_score pamphlet post post-weblog speech webpage broadcast" match="any">
          <group delimiter=", ">
            <text macro="locators" prefix="" suffix=""/>
            <text macro="secondary-contributors" prefix="" suffix=""/>
          </group>
        </else-if>
        <else-if type="article dataset figure" match="any">
          <choose>
            <if variable="container-title">
              <text macro="secondary-contributors" prefix="" suffix=""/>
            </if>
            <else>
              <group delimiter=", ">
                <text macro="locators" prefix="" suffix=""/>
                <text macro="secondary-contributors" prefix="" suffix=""/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if type="bill legislation legal_case patent treaty personal_communication" match="none">
          <text macro="secondary-contributors" prefix="" suffix=""/>
        </else-if>
      </choose>
    </group>
  </macro>
  <macro name="format">
    <group>
      <choose>
        <if variable="reviewed-author reviewed-title" type="review review-book" match="any">
          <group delimiter=", ">
            <names variable="reviewed-author" prefix="&lt;reviewed-author>" suffix="&lt;/reviewed-author>"/>
          </group>
        </if>
        <else>
          <text macro="format-short" prefix="" suffix=""/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="format-short">
    <choose>
      <if type="speech thesis" match="any">
        <text variable="medium" text-case="capitalize-first" prefix="&lt;medium>" suffix="&lt;/medium>"/>
      </if>
      <else-if type="book report" match="any">
        <choose>
          <if variable="container-title" match="none">
            <text macro="format-report" prefix="" suffix=""/>
          </if>
        </choose>
      </else-if>
      <else-if type="manuscript pamphlet" match="any">
        <text variable="medium" text-case="capitalize-first" prefix="&lt;medium>" suffix="&lt;/medium>"/>
      </else-if>
      <else-if type="personal_communication">
        <text macro="secondary-contributors" prefix="" suffix=""/>
      </else-if>
    </choose>
  </macro>
  <macro name="description-report">
    <choose>
      <if variable="issue" match="any">
        <group delimiter=", ">
          <group delimiter=" " suffix=",">
            <text variable="issue" prefix="&lt;issue>" suffix="&lt;/issue>"/>
          </group>
          <text macro="locators" prefix="" suffix=""/>
        </group>
      </if>
      <else>
        <text macro="locators" prefix="" suffix=""/>
      </else>
    </choose>
  </macro>
  <macro name="format-report">
    <choose>
      <if variable="number">
        <text variable="medium" text-case="lowercase" suffix="&lt;/medium>." prefix="&lt;medium>"/>
      </if>
      <else>
        <group delimiter=", ">
          <text variable="genre" text-case="capitalize-first" prefix="&lt;genre>" suffix="&lt;/genre>"/>
          <text variable="medium" text-case="capitalize-first" prefix="&lt;medium>" suffix="&lt;/medium>"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="archive">
    <group delimiter=". ">
      <group delimiter=", ">
        <choose>
          <if type="manuscript">
            <text variable="genre" prefix="&lt;genre>" suffix="&lt;/genre>"/>
          </if>
        </choose>
        <group delimiter=" ">
          <text variable="archive" prefix="&lt;archive>" suffix="&lt;/archive>"/>
          <text variable="archive_location" prefix="(&lt;archive_location>" suffix="&lt;/archive_location>)"/>
        </group>
      </group>
      <group delimiter=", ">
        <text variable="archive-place" prefix="&lt;archive-place>" suffix="&lt;/archive-place>"/>
      </group>
    </group>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="manuscript pamphlet" match="any">
        <choose>
          <if variable="archive archive_location archive-place" match="any">
            <group delimiter=". ">
              <group delimiter=": ">
                <text variable="publisher-place" prefix="&lt;publisher-place>" suffix="&lt;/publisher-place>"/>
                <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
              </group>
              <text macro="archive" prefix="" suffix=""/>
            </group>
          </if>
          <else>
            <group delimiter=", ">
              <text variable="genre" prefix="&lt;genre>" suffix="&lt;/genre>"/>
              <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
              <text variable="publisher-place" prefix="&lt;publisher-place>" suffix="&lt;/publisher-place>"/>
            </group>
          </else>
        </choose>
      </if>
      <else-if type="thesis" match="any">
        <group delimiter=". ">
          <group delimiter=", ">
            <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
          </group>
          <text macro="archive" prefix="" suffix=""/>
        </group>
      </else-if>
      <else-if type="patent">
        <group delimiter=". ">
          <group delimiter=": ">
            <text variable="publisher-place" prefix="&lt;publisher-place>" suffix="&lt;/publisher-place>"/>
            <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
          </group>
          <text macro="archive" prefix="" suffix=""/>
        </group>
      </else-if>
      <else-if type="article-journal article-magazine article-newspaper" match="any">
        <text macro="archive" prefix="" suffix=""/>
      </else-if>
      <else-if type="post post-weblog webpage broadcast speech" match="none">
        <group delimiter=" ">
          <choose>
            <if match="any" variable="page">
              <group delimiter=": ">
                <text variable="publisher-place" prefix="&lt;publisher-place>" suffix="&lt;/publisher-place>"/>
                <text variable="publisher" suffix="&lt;/publisher>," prefix="&lt;publisher>"/>
              </group>
              <group>
                <label suffix=" " variable="page" form="short"/>
                <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
              </group>
            </if>
            <else>
              <group delimiter=": ">
                <text variable="publisher-place" prefix="&lt;publisher-place>" suffix="&lt;/publisher-place>"/>
                <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
              </group>
            </else>
          </choose>
          <text macro="archive" prefix="" suffix=""/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="issued">
