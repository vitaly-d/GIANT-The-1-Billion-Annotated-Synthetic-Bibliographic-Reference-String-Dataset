                    </if>
                  </choose>
                </if>
              </choose>
            </else-if>
            <else>
              <group>
                <text term="volume" form="short" text-case="lowercase" suffix=" " prefix=""/>
                <number variable="volume" form="numeric" suffix=","/>
              </group>
            </else>
          </choose>
        </if>
        <else>
          <group>
            <text term="volume" form="short" plural="true" text-case="capitalize-first" suffix=" " prefix=""/>
            <number variable="number-of-volumes" form="numeric" prefix="1â€“"/>
          </group>
        </else>
      </choose>
    </group>
