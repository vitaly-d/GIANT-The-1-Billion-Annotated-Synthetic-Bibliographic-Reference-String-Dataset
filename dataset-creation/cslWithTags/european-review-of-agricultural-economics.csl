          <choose>
            <if variable="references">
              <text variable="references" prefix="&lt;references>" suffix="&lt;/references>"/>
            </if>
            <else>
              <group delimiter=" ">
                <text value="Original work published" prefix="" suffix=""/>
                <choose>
                  <if is-uncertain-date="original-date">
                    <text term="circa" form="short" prefix="" suffix=""/>
                  </if>
                </choose>
                <date variable="original-date" prefix="&lt;original-date>" suffix="&lt;/original-date>">
                  <date-part name="year" prefix="&lt;year>" suffix="&lt;/year>"/>
                </date>
              </group>
            </else>
          </choose>
        </group>
      </if>
      <else>
        <text variable="references" prefix="(&lt;references>" suffix="&lt;/references>)"/>
      </else>
    </choose>
  </macro>
  <macro name="citation-locator">
    <group delimiter=" ">
      <choose>
        <if locator="chapter">
          <label variable="locator" text-case="capitalize-first"/>
        </if>
      </choose>
      <text variable="locator" strip-periods="false" prefix=": &lt;locator>" suffix="&lt;/locator>"/>
    </group>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" disambiguate-add-names="true" disambiguate-add-givenname="true" collapse="year" givenname-disambiguation-rule="primary-name">
    
    <layout prefix="(" suffix=")" delimiter="; ">
      <group>
        <text macro="author-intext" suffix=", " prefix=""/>
        <text macro="date-intext" prefix="" suffix=""/>
        <text macro="citation-locator" prefix="" suffix=""/>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true" et-al-min="21" et-al-use-first="19" et-al-use-last="true" entry-spacing="0" line-spacing="2">
    
    <layout>
      <group delimiter=" ">
        <group delimiter=". " suffix=".">
          <text macro="author-bib" prefix="" suffix=""/>
          <text macro="date-bib" prefix="" suffix=""/>
          <text macro="title-and-descriptions" prefix="" suffix=""/>
          <text macro="container" prefix="" suffix=""/>
          <text macro="event" prefix="" suffix=""/>
          <text macro="publisher" prefix="" suffix=""/>
        </group>
        <text macro="access" prefix="" suffix=""/>
